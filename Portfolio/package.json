{
  "name": "personal-portfolio",
  "version": "1.0.1",
  "scripts": {
    "serve": "npx eleventy --serve",
    "css:watch": "npx tailwindcss -i ./tailwind.css -o ./_site/css/styles.css --watch",
    "dev": "npm-run-all --parallel serve css:watch"
  },

  "devDependencies": {
    "@11ty/eleventy": "github:11ty/eleventy",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.3"
  }
}
